{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
  {{ if ne .Params.unlisted true }}
    {{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "categories" .Params.categories "contents" .Plain "permalink" .Permalink "image" .Params.image) -}}
  {{- end -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}